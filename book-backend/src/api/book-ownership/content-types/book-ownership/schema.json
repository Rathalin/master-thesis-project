{
  "kind": "collectionType",
  "collectionName": "book_ownerships",
  "info": {
    "singularName": "book-ownership",
    "pluralName": "book-ownerships",
    "displayName": "BookOwnership",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "book": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::book.book"
    },
    "startReading": {
      "type": "date"
    },
    "finishReading": {
      "type": "date"
    },
    "rating": {
      "type": "enumeration",
      "enum": ["No Rating", "Very bad", "Bad", "Ok", "Good", "Very Good"]
    },
    "currentPage": {
      "type": "integer",
      "min": 0
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "note": {
      "type": "richtext",
      "maxLength": 1000
    },
    "order": {
      "type": "integer",
      "min": 0,
      "unique": false,
      "required": false
    }
  }
}
