{
  "name": "cat-gallery",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "src/apps/cat-gallery/src",
  "prefix": "cat-gallery",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/src/apps/cat-gallery",
        "index": "src/apps/cat-gallery/src/index.html",
        "main": "src/apps/cat-gallery/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "src/apps/cat-gallery/tsconfig.app.json",
        "assets": [
          "src/apps/cat-gallery/src/favicon.ico",
          "src/apps/cat-gallery/src/assets"
        ],
        "styles": ["src/apps/cat-gallery/src/styles.css"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "src/apps/cat-gallery/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "src/apps/cat-gallery/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-dev-server",
      "configurations": {
        "production": {
          "browserTarget": "cat-gallery:build:production"
        },
        "development": {
          "browserTarget": "cat-gallery:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4203,
        "publicHost": "http://localhost:4203"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "cat-gallery:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "src/apps/cat-gallery/**/*.ts",
          "src/apps/cat-gallery/**/*.html"
        ]
      }
    },
    "serve-static": {
      "executor": "@nrwl/angular:file-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "cat-gallery:build",
        "port": 4203
      },
      "configurations": {
        "development": {
          "buildTarget": "cat-gallery:build:development"
        },
        "production": {
          "buildTarget": "cat-gallery:build:production"
        }
      }
    }
  },
  "tags": []
}
